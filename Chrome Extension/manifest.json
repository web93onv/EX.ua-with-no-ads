{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "script.js" ],
      "matches": [ "http://www.ex.ua/*" ],
      "run_at": "document_start"
   } ],
   "description": "Блокировка назойливой рекламы на EX.ua",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "EX.ua без рекламы!",
   "options_page": "options.html",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "page_action": {
      "default_icon": {
         "19": "icon19.png",
         "38": "icon38.png"
      },
      "default_popup": "options.html",
      "default_title": "Показать настройки ''EX.ua без рекламы!''"
   },
   "permissions": [ "http://www.ex.ua/*", "activeTab", "storage" ],
   "version": "1.8.13"
}
